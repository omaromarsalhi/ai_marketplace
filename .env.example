# API Configuration
NUXT_API_SECRET=your-secret-key-here

# Public Configuration
NUXT_PUBLIC_API_BASE=/api

# Ollama Cloud Configuration
OLLAMA_PROVIDER=cloud
OLLAMA_API_KEY=your_ollama_cloud_api_key_here

# Model Configuration
# Text Models
OLLAMA_CLOUD_TEXT_MODEL=qwen2.5:3b

# Vision Models  
OLLAMA_CLOUD_VISION_MODEL=llava:7b-v1.6-mistral-q2_K

# Vision Provider Configuration
# Choose: 'gemini' for Google Gemini, 'ollama' for Ollama vision models
# Recommended: 'gemini' for better performance with Ollama Cloud
VISION_PROVIDER=gemini

# Google Gemini API Key (required when VISION_PROVIDER=gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# Example configurations:
# 
# Configuration 1: Ollama Cloud + Gemini Vision (recommended)
# OLLAMA_PROVIDER=cloud
# OLLAMA_API_KEY=your_ollama_cloud_key
# OLLAMA_CLOUD_TEXT_MODEL=qwen2.5:3b
# OLLAMA_CLOUD_VISION_MODEL=llava:7b-v1.6-mistral-q2_K
# VISION_PROVIDER=gemini
# GEMINI_API_KEY=your_gemini_key
#
# Configuration 2: Full Ollama Cloud (vision + text)
# OLLAMA_PROVIDER=cloud
# OLLAMA_API_KEY=your_ollama_cloud_key
# OLLAMA_CLOUD_TEXT_MODEL=qwen2.5:3b
# OLLAMA_CLOUD_VISION_MODEL=llava:7b-v1.6-mistral-q2_K
# VISION_PROVIDER=ollama
#
# Configuration 3: High Performance (better models)
# OLLAMA_PROVIDER=cloud
# OLLAMA_API_KEY=your_ollama_cloud_key
# OLLAMA_CLOUD_TEXT_MODEL=llama3:8b
# OLLAMA_CLOUD_VISION_MODEL=llava:7b-v1.6-mistral-q2_K
# VISION_PROVIDER=gemini
# GEMINI_API_KEY=your_gemini_key
